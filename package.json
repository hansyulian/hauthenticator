    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^48.0.2",
    "module-resolver": "^1.0.0",
    "prettier": "^3.1.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.4"
  },
