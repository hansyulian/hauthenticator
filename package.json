{
  "name": "hauthenticator",
  "version": "0.10.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc",
    "test:ci": "npm run test --coverage",
    "test:badges": "npm run test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "proto:compile": "node scripts/compileProto",
    "build:android:development": "eas build -p android --profile development",
    "build:android:apk": "eas build -p android --profile apk",
    "build:android:local-apk": "eas build -p android --profile apk --local",
    "build:android:local-dev": "eas build -p android --profile development --local"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
    "@expo-google-fonts/nunito": "^0.2.3",
    "@googleapis/drive": "^8.3.1",
