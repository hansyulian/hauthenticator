    "@otplib/plugin-base32-enc-dec": "^12.0.1",
    "@otplib/plugin-crypto-js": "^12.0.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-navigation/native": "6.1.7",
    "@react-navigation/native-stack": "6.9.13",
    "@robinbobin/react-native-google-drive-api-wrapper": "^1.2.3",
    "axios": "^1.5.0",
    "crypto-js": "3.3.0",
    "expo": "48.0.18",
    "expo-barcode-scanner": "~12.3.2",
    "expo-clipboard": "~4.1.2",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-google-drive-api-wrapper": "^1.0.2",
    "expo-local-authentication": "~13.3.0",
    "expo-secure-store": "~12.1.1",
    "expo-status-bar": "1.4.4",
    "md5": "^2.3.0",
    "protobufjs": "^7.2.4",
    "react": "18.2.0",
    "react-native": "0.71.14",
    "react-native-fast-base64": "^0.1.2",
